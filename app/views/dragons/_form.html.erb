<%= simple_form_for @dragon do |f| %>
  <%= f.input :name %>
  <%= f.input :age %>
  <%= f.input :location %>
  <%= f.input :category,collection: Dragon::CATEGORY %>
  <%= f.input :price_per_day %>
  <%= f.input :description %>
  <%= f.input :seats %>
  <%= f.submit "Confirm", class: "btn btn-primary rounded w-100" %>
<% end %>
